   "source": [
    "import types \n",
    "\n",
    "def func() :\n",
    "    print(\" 함수 \")\n",
    "    \n",
    "if type(func) == types.FunctionType :\n",
    "    print(\" 함수 클래스의 객체 입니다. \")\n",
    "    func()\n",
    "    \n",
    "if type(lambda : print(\"람다\")) == types.LambdaType :\n",
    "     print(\" 람다 표현식 \")\n",
    "     (lambda : print(\" 람다\"))()"
   ]
